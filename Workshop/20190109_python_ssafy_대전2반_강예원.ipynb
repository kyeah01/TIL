    }
   ],
   "source": [
    "def my_sqrt(n):\n",
    "    a=0\n",
    "    b=n*2\n",
    "    for i in range(50):\n",
    "        c = (a+b)/2\n",
    "        if c**2 > n:\n",
    "            b = c\n",
    "        else:\n",
    "            a = c\n",
    "    return (a+b)/2\n",
    "\n",
    "my_sqrt(4)"
   ]
  }
